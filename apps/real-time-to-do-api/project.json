{
	"name": "real-time-to-do-api",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/real-time-to-do-api/src",
	"projectType": "application",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/rspack:rspack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "development",
			"options": {
				"target": "node",
				"outputPath": "dist/apps/real-time-to-do-api",
				"main": "apps/real-time-to-do-api/src/main.ts",
				"tsConfig": "apps/real-time-to-do-api/tsconfig.app.json",
				"rspackConfig": "apps/real-time-to-do-api/rspack.config.js"
			},
			"configurations": {
				"development": {
					"mode": "development"
				},
				"production": {
					"mode": "production",
					"sourceMap": false
				}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "real-time-to-do-api:build"
			},
			"configurations": {
				"development": {
					"buildTarget": "real-time-to-do-api:build:development"
				},
				"production": {
					"buildTarget": "real-time-to-do-api:build:production"
				}
			}
		}
	}
}
